<table class="table table-striped track-list">
  <thead>
    <tr>
      <th>Track</th>
      <th>Artist</th>
      <th>Time</th>
      <th>Album</th>
      <th>Year</th>
      <th>Actions</th>
    </tr>
  </head>
  <tbody>
    {{#each view.content}}
      {{#view view.itemView contentBinding="this"}}
        <td>{{view.content.name}}</td>
        <td>{{view.content.artistName}}</td>
        <td>{{view.content.displayDuration}}</td>
        <td>{{view.content.album.name}}</td>
        <td>{{view.content.album.released}}</td>
        <td><a class="btn btn-success" {{action "enqueue"}}>Enqueue</a></td>
      {{/view}}
    {{/each}}
  </tbody>
</table>
