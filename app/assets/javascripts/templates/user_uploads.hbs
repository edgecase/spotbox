<table class="table table-striped track-list">
  {{#if showContent}}
    <thead>
      <tr>
        <th>Track</th>
        <th>Artist</th>
        <th>Time</th>
        <th>Album</th>
        <th>Year</th>
        <th></th>
      </tr>
    </head>
    <tbody>
      {{#each content}}
      {{/each}}
    </tbody>
  {{/if}}
</table>
